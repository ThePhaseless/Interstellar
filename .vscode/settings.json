{
  "files.associations": {
    "./Config/Home/HomeAssistant/*.yaml": "home-assistant",
    "./Ansible/**/*.yaml": "ansible",
    "./Ansible/**/*.yml": "ansible"
  },
  "yaml.schemas": {
    "https://www.authelia.com/schemas/v4.38/json-schema/configuration.json": "./Config/Auth/authelia.yaml"
  },
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.activateEnvironment": true,
  "cSpell.words": [
    "AUTHKEY",
    "loadbalancer",
    "objectstorage",
    "OCID",
    "tflint",
    "traefik"
  ],
  "yaml.customTags": ["!env_var", "!include_dir_merge_named"],
  "python.defaultInterpreterPath": "./.venv/bin/python",
  "ansible.python.interpreterPath": "/workspaces/Interstellar/.venv/bin/python"
}
